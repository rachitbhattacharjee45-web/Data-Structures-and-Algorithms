#include <stdio.h>
#include <stdlib.h>

typedef struct Node {
    int data;
    struct Node* prev;
    struct Node* next;
} Node;

int main() {
    Node* head = (Node*)malloc(sizeof(Node));
    Node* second = (Node*)malloc(sizeof(Node));
    Node* third = (Node*)malloc(sizeof(Node));

    if (head != NULL && second != NULL && third != NULL) {
        head->data = 10;
        head->prev = NULL;
        head->next = second;

        second->data = 20;
        second->prev = head;
        second->next = third;

        third->data = 30;
        third->prev = second;
        third->next = NULL;
    } else {
        printf("Memory Allocation Failed.\n");
        return 1;
    }
    if (head != NULL) {
        Node* temp = head;
        head = head->next;
        if (head != NULL) {
            head->prev = NULL;
        }
        free(temp);
    }
    printf("Linked List after deleting first node: ");
    Node* current = head;
    while (current != NULL) {
        printf("%d ", current->data);
        current = current->next;
    }
    printf("\n");
    Node* temp;
    while (head != NULL) {
        temp = head;
        head = head->next;
        free(temp);
    }

    return 0;
}
